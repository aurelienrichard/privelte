{
	"name": "privelte",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"clean": "rm -rf .turbo && rm -rf coverage && rm -rf .wrangler",
		"dev": "vite dev",
		"lint": "prettier . --check . && eslint . --report-unused-disable-directives",
		"deploy": "wrangler pages deploy ./.svelte-kit/cloudflare --project-name privelte",
		"functions": "supabase functions deploy --project-ref yipoupsiomqlwjtapqda",
		"logs:preview": "wrangler pages deployment tail --project-name privelte --environment preview",
		"logs:production": "wrangler pages deployment tail --project-name privelte --environment production",
		"preview": "vite preview",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"test": "vitest run",
		"update-types": "supabase gen types typescript --project-id yipoupsiomqlwjtapqda --schema public > src/lib/types/supabase.ts"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.9.2",
		"@skeletonlabs/tw-plugin": "^0.3.1",
		"@sveltejs/adapter-cloudflare": "^4.4.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "^0.5.7",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/svelte": "^4.2.3",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^20.12.8",
		"autoprefixer": "^10.4.19",
		"happy-dom": "^13.10.1",
		"postcss": "^8.4.38",
		"supabase": "^1.165.0",
		"svelte": "^4.2.15",
		"svelte-check": "^3.7.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-tailwind-purgecss": "^0.2.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^2.43.1",
		"jose": "^5.2.4",
		"nanoid": "^5.0.7",
		"unique-names-generator": "^4.7.1",
		"zod": "^3.23.6"
	}
}
